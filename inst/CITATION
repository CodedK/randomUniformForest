year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{randomUniformForest}: random Uniform Forests for Classification and Regression",
         author = c(person("Saip", "Ciss")),
         year = year,
         note = note,
         url = "http://CRAN.R-project.org/package=randomUniformForest")